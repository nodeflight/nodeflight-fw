/* CPU: STM32F745xG */

/*
Extra RAM for real time data:
TCM_RAM (xrw)  : ORIGIN = XXXXXXXXXX, LENGTH = 32K
*/

MEMORY
{
RAM (xrw)         : ORIGIN = 0x20000000, LENGTH = 256K
FLASH_APP (rx)    : ORIGIN = 0x08000000, LENGTH = 1024K - 128K
FLASH_L1CONF (rx) : ORIGIN = 0x080e0000, LENGTH = 128K
}

INCLUDE "STM32.ld"